<% provide(:title, "Edit profile #{current_user.name}") %>
<h1 class="nice_heading"><%= @user.name %></h1>
<hr/>

<div class="row", style="margin-top: 2em;">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@user) do |f| %>
      <%= render 'email_form', f: f %>
    <% end %>
  </div>
</div>
