<% provide(:title, "New") %>
<h1 class="center">Save a new flower</h1>

<div class="row">
	<div class="col-md-6 col-md-offset-3">
		<%= form_for(@flower) do |f| %>
			<%= f.label :name %>
			<%= f.text_field :name %>

			<%= f.label :bot_name, "Botanical name" %>
			<%= f.text_field :bot_name %>

			<%= f.label :description %>
			<%= f.text_area :description %>

			<%= f.submit "Save", class: "btn btn-primary" %>
		<% end %>
	</div>
</div>
